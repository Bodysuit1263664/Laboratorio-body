#!/data/data/com.termux/files/usr/bin/bash
#Coded by @Ivam3 on January 2019
#Contact: https://t.me/Ivam3
IFS=$'\n\t'
source $PREFIX/share/i-Haklab/.set/var/variables
source $PREFIX/share/i-Haklab/.set/functions/functions
chk-iHaklab

printf "$G
   ___  _____  __  _______           __
  / _ )/  _/ |/ / / ___/ /  By_Ivam3/ /_____ ____
 / _  |/ //    / / /__/ _ \/ -_) __/  '_/ -_) __/
/____/___/_/|_/  \___/_//_/\__/\__/_/\_\\__/_/$W v.1.0

$Y"
printf "$Y(_>)─➤$G Input BIN $W"
while read -p "(_>)─➤ " BIN && [ -z $BIN ]; do
	printf "\n$R(_>)─➤$W Please set a BIN\n"
done
printf "\n$Y(_>)─➤$G Checking..\n\n"
if [ $BIN ]
then
curl -H "Accept-Version: 3" "https://lookup.binlist.net/$BIN"
sleep 1
fi
